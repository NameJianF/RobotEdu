<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="295.0"
            prefWidth="410.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="robot.client.controller.LoginController">
    <children>
        <Label layoutX="85.0" layoutY="72.0" text="用户名"/>
        <TextField fx:id="txtUserName" layoutX="155.0" layoutY="68.0" text=""/>

        <Label layoutX="85.0" layoutY="126.0" text="密码"/>
        <PasswordField fx:id="txtPwd" layoutX="155.0" layoutY="122.0" text=""/>

        <Button fx:id="btnLogin" layoutX="85.0" layoutY="189.0" mnemonicParsing="false"
                onMouseClicked="#loginClick" prefHeight="23.0" prefWidth="96.0" text="登录"/>
        <Button fx:id="btnCancel" layoutX="220.0" layoutY="189.0" mnemonicParsing="false"
                onMouseClicked="#cancelClick" prefHeight="23.0" prefWidth="96.0" text="取消"/>
    </children>
</AnchorPane>
